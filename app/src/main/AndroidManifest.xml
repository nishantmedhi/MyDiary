<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nmedh.mynote">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".DisplayWishesActivity"
            android:label="Wishes" />
        <activity
            android:name=".WishDetailActivity"
            android:label="Wish Details" />
        <activity android:name=".CreatePassword" />
        <activity android:name=".EnterPassword" />
        <activity android:name=".Settings" />
        <activity android:name=".Login" />
        <activity android:name=".Reminder">
        </activity>
        <receiver android:name=".MyAlarm"
                    android:enabled="true"
                    android:exported="true"
                    android:process=":remote"

            />
    </application>

</manifest>